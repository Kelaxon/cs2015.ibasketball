<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="AboutAndroid" namespace="/" extends="json-default">

		<action name="UserActionApp" class="JsonAction.UserAction"
			method="login">
			<result type="json">
				<param name="includeProperties">message, currentUser.*</param>
			</result>
		</action>
		<action name="UserGetNewsApp" class="JsonAction.NewsAction"
			method="getAll">
			<result type="json">
				<param name="includeProperties">message, newsList.* </param>
			</result>
		</action>
		<!-- <param name="root">list</param> -->
		<!-- <param name="excludeNullProperties">true</param> -->
		<!-- <param name="includeProperties">list.* </param> -->
		<!-- <param name="excludeProperties">id</param> -->

	</package>

	<package name="AboutAllUsers" extends="struts-default">
		<action name="DebugAction" class="Action.DebugAction">
			<result name="input" type="redirect">/200.jsp</result>
			<result name="error">/404.jsp</result>
		</action>

		<action name="UsersLoginAction" class="Action.AllUsersLoginAction"
			method="login">
			<result name="user">/userNewsIndex.jsp</result>
			<result name="manager">/managerNewsIndex.jsp</result>
			<result name="input">/userNewsIndex.jsp</result>
			<result name="error">/404.jsp</result>
		</action>
		<action name="UsersLogoutAction" class="Action.AllUsersLoginAction"
			method="logout">
			<result name="success">/userNewsIndex.jsp</result>
			<result name="error">/404.jsp</result>
		</action>

		<action name="FirstEntryLoadDataAction" class="Action.FirstEntryLoadDataAction">
			<result name="success">/userNewsIndexload.jsp</result>
		</action>
	</package>

	<package name="AboutManager" extends="struts-default">
		<action name="ManagerCDUNewsAction" class="Action.ManagerCDUNewsAction">
			<result name="success">/managerNewsIndex.jsp</result>
			<result name="error">/404.jsp</result>
		</action>
		<action name="ManagerDeleteMessageAction" class="Action.ManagerDMessageAction"
			method="delete">
			<result name="success">/managerMessageIndex.jsp</result>
			<result name="error">/404.jsp</result>
		</action>
		<action name="ManagerCDUserAction" class="Action.ManagerCDUserAction">
			<result name="success">/managerUsersIndex.jsp</result>
			<result name="error">/404.jsp</result>
		</action>
	</package>

	<package name="AboutUser" extends="struts-default">
		<action name="UserUInfoAction" class="Action.UserUInfoAction">
			<result name="success">/userInfoIndex.jsp</result>
			<result name="error">/404.jsp</result>
		</action>
		<action name="UserCDMessageAction" class="Action.UserCDMessageAction">
			<result name="success">/userMessageIndex.jsp</result>
			<result name="error">/404.jsp</result>
		</action>
		<action name="UserCDCollectionAction" class="Action.UserCDCollectionAction">
			<result name="success">/userCollectionIndex.jsp</result>
			<result name="error">/404.jsp</result>
		</action>
	</package>

